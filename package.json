{
  "name": "@tetu_io/tetu-contracts-v2",
  "version": "2.0.0",
  "description": "TETU solidity base contracts V2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "contracts": "contracts",
    "dist": "dist"
  },
  "scripts": {
    "build-module": "npm run build && tsc",
    "prepare": "npm run build-module",
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npm run build && npx hardhat test",
    "coverage": "npm run build && npx cross-env NODE_OPTIONS=\"--max_old_space_size=8192\" hardhat coverage --temp artifacts --max-memory 4096",
    "slither": "npm run build && slither .",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tetu-io/tetu-contracts-v2.git"
  },
  "author": "TETU team <admin@tetu.io>",
  "license": "MIT",
  "homepage": "https://tetu.io",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@nomiclabs/hardhat-solhint": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.14",
    "axios": "^1.2.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.22",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^1.2.0",
    "hardhat-typechain": "^0.3.5",
    "mocha": "^10.2.0",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslog": "^4.4.4",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  },
  "engines": {
    "node": "^14.18.1"
  }
}
